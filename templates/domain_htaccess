Header Set Access-Control-Allow-Origin "https://api.{{domain}}"
Header Set Access-Control-Allow-Methods "GET, POST, OPTIONS"
Header Set Access-Control-Allow-Headers "DNT,X-CustomHeader,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Content-Range,Range"
Header Set Access-Control-Max-Age "1728000"
Header Set X-XSS-Protection "1; mode=block"
Header Set X-Content-Type-Options "nosniff"
Header Set Cross-Origin-Resource-Policy "cross-origin"
Header Set Cross-Origin-Embedder-Policy "require-corp"
Header Set Content-Security-Policy "default-src 'none'; child-src https://{{domain}}; worker-src 'self'; media-src blob:; style-src 'unsafe-inline' 'self' https://{{domain}}; script-src 'self' resource: https://{{domain}}; connect-src 'self' blob: https://{{domain}} https://api.{{domain}} wss://{{domain}}; font-src 'self' data: https://{{domain}}; img-src 'self' data: blob: https://{{domain}}; frame-src 'self' blob: https://api.{{domain}}; frame-ancestors 'self' https://{{domain}};" "expr=%{REQUEST_URI} !~ m#^/(sheet|common)/#"
